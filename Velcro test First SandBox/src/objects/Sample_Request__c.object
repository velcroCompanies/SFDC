    <validationRules>
        <fullName>Address_account_changed_check</fullName>
        <active>true</active>
        <errorConditionFormula>(ISBLANK(PRIORVALUE(Customer_Shipping_Address__c )) 
&amp;&amp; NOT(ISBLANK(Customer_Shipping_Address__c ))
)
&amp;&amp; NOT(ISBLANK( VELCRO_shipping_address__c))</errorConditionFormula>
        <errorMessage>First remove the Affiliate address before adding customer delivery information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Address_check</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK( VELCRO_shipping_address__c ) &amp;&amp;
ISBLANK(Customer_Shipping_Address__c )</errorConditionFormula>
        <errorMessage>You must select either an Affiliate or provide the customer address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Address_velcro_changed_check</fullName>
        <active>true</active>
        <errorConditionFormula>(ISBLANK(PRIORVALUE(VELCRO_shipping_address__c)) 
&amp;&amp; NOT(ISBLANK(VELCRO_shipping_address__c ))
)
&amp;&amp; NOT(ISBLANK( Customer_Shipping_Address__c))</errorConditionFormula>
        <errorMessage>First remove the Customer shipping  address before adding VELCRO Affiliate delivery information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Method_check</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Customer_Shipping_Address__c ))
&amp;&amp; ISPICKVAL( Delivery_method__c , &apos;VELCRO TRUCK&apos;)</errorConditionFormula>
        <errorMessage>You can not use &apos;Velcro truck&apos; delivery method to send samples to a customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UPS_name_and_contact</fullName>
        <active>false</active>
        <description>If you choose UPS to customer , you must provide email and phone numbers</description>
        <errorConditionFormula>ISPICKVAL( Delivery_method__c ,&apos;UPS&apos;) 
&amp;&amp;
ISBLANK(VELCRO_shipping_address__c )
&amp;&amp; ( ISBLANK( Customer_Email__c ) 
||  ISBLANK(  Customer_Phone__c  ) )</errorConditionFormula>
        <errorMessage>If you choose UPS delivery , you must provide email and phone numbers</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>New_Request</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}  
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}  

var status=&apos;{!Sample_Request__c.Status__c}&apos;;

if (status==&apos;Approved&apos;) {
var result=&apos;/apex/SDOC__SDCreate1?id={!Sample_Request__c.Id}&amp;Object=Sample_Request__c&apos;;
location.href=result;
}
else
{
  alert(&apos;Request has not yet been approved.&apos;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Send_to_assigned_user</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send to assigned user</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

var template=&apos;&apos;;

template = &quot;&amp;template_id=00XA0000001fdGI&quot;;


//window.location.reload();
var url=&apos;/email/author/emailauthor.jsp?retURL=/{!Sample_Request__c.Id}&amp;p3_lkid={!Sample_Request__c.Id}&amp;p2_lkid={!Sample_Request__c.Assigned__c}&amp;p4=&amp;p24={!$User.Email}&apos;+template;

location.replace(url);</url>
    </webLinks>
    <webLinks>
        <fullName>Set_Shipping_Address</fullName>
        <availability>online</availability>
        <description>Sets the Shipping Address to the existing Account Shipping address stored in the Account_shipping_address__c field.</description>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set Shipping Address (using Account data)</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)}
 
var SamplesUpdate = new sforce.SObject(&quot;Sample_Request__c&quot;);

SamplesUpdate.Id=&apos;{!Sample_Request__c.Id}&apos;;
SamplesUpdate.Customer_Shipping_Address__c=
&apos;{!Sample_Request__c.Account_shipping_address__c}&apos;;
 

var result=updateSample = sforce.connection.update([SamplesUpdate]);
 
if (result[0].success==&apos;false&apos;) { 
alert(result[0].errors.message); 
} else { 
window.parent.location.href=&quot;/{!Sample_Request__c.Id}&quot;; 
}</url>
    </webLinks>
