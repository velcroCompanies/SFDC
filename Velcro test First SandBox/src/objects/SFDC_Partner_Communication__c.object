    </fields>
    <fields>
        <fullName>Content_Image__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Image_Resource_ID__c &lt;&gt; &quot;&quot;,  	HYPERLINK(URL__c,  		IMAGE(&quot;/servlet/servlet.FileDownload?file=&quot; &amp; Image_Resource_ID__c, &quot;View Content&quot;, 100,150 )), &quot;&quot;)</formula>
