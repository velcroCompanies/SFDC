        <label>Created User Region</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DB_Activity_Type__c</fullName>
        <description>This is a field that is installed by and used with the Adoption Dashboard AppExchange package.  If your org already has a similar field, you can change the reports that are part of the Adoption Dashboard package to use your custom field and then delete this field.</description>
        <externalId>false</externalId>
        <formula>IF(CONTAINS( Subject , &quot;Email&quot;), &quot;Email&quot;, 
IF(CONTAINS( Subject , &quot;E-Mail&quot;), &quot;Email&quot;,
IF(CONTAINS( Subject , &quot;Cold Call&quot;), &quot;Cold Call&quot;, 
IF(CONTAINS( Subject , &quot;Call&quot;), &quot;Call&quot;, 
IF(CONTAINS( Subject , &quot;Meeting&quot;), &quot;Meeting&quot;, 
IF(CONTAINS( Subject , &quot;On Site Visit&quot;), &quot;Site Visit&quot;, 
IF(CONTAINS( Subject , &quot;Quote&quot;), &quot;Send Letter/Quote&quot;,
IF(CONTAINS( Location, &quot;zoom&quot;),&quot;Zoom Meeting&quot;, 
IF(NOT(ISBLANK(Location)), &quot;On Site Visit&quot;, 
&quot;Other&quot;
)))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DB Activity Type</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
