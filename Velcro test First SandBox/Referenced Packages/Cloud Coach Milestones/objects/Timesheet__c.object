    <fields>
        <fullName>End__c</fullName>
        <deprecated>false</deprecated>
        <description>The end of the Timesheet is 7 days (including the start date) after the start of the Timesheet</description>
        <externalId>false</externalId>
        <formula>Start__c + 6</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The end of the Timesheet is 7 days (including the start date) after the start of the Timesheet</inlineHelpText>
        <label>End</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Hash__c</fullName>
        <deprecated>false</deprecated>
        <description>This is a hash of the OwnerId of the Timesheet and the start date, it is used to enforce unique timesheets</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(OwnerId) + TEXT(YEAR(Start__c)) + &apos;-&apos; + TEXT(MONTH(Start__c)) + &apos;-&apos; + TEXT(DAY(Start__c))</formula>
        <inlineHelpText>This is a system field and should never be visible.</inlineHelpText>
        <label>Hash</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Start__c</fullName>
        <deprecated>false</deprecated>
        <description>The date the Timesheet entries can begin</description>
        <externalId>false</externalId>
        <inlineHelpText>The first day of the Timesheet</inlineHelpText>
        <label>Start</label>
        <required>true</required>
        <trackHistory>false</trackHistory>
