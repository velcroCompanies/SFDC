    <fields>
        <fullName>Start__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(Completed_Date__c &lt;= Desired_Start__c || Completed_Date__c &lt;= Parent_End__c,
  Completed_Date__c,
  IF(ISBLANK(Parent_End__c) || Desired_Start__c &gt; Parent_End__c,
    /* Use desired start if after parent end (or if there is no parent) */
    Desired_Start__c,
    /* Otherwise, set to Parent End + 1, adjusted for weekends */
    Parent_End__c + CASE(MOD(Parent_End__c - DATE(1900, 1, 8), 7),
      4, 3,
      5, 2,
      1
    )
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The calculated start date of the Task, derived from the Desired Start Date and, if there is a parent Task, the parent Task&apos;s calculated end date.</inlineHelpText>
        <label>Calculated Start Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
