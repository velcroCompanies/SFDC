    <fields>
        <fullName>Calculated_Duration__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Duration__c +
/* Add 2 days for each full week */
(FLOOR(Duration__c / 5) * 2) +
/* Determine the day of the week that the task is scheduled to end on, and adjust for the next working day
   5/6: Weekend, add an additional 2 days
   -1: Started Monday and duration in full weeks, subtract 2 extra days added from full week calculation */
CASE(MOD(Start__c - DATE(1900, 1, 8), 7) + MOD(Duration__c, 5) - 1,
  5, 2,
  6, 2,
  -1, -2,
  0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The length of the Task when weekend days are taken into account.</inlineHelpText>
        <label>Calculated Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
