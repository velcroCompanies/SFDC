    <fields>
        <fullName>Timeline_Health_Reason__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CASE(Status_Enum__c,
  2, &apos;Task is complete&apos;,
  -1, IF(ISPICKVAL(Status__c, &apos;On Hold&apos;), &apos;Task is on hold&apos;, &apos;&apos;),
  IF(End__c &lt; TODAY(),
    &apos;Task is ending late&apos;,
    IF(Status_Enum__c == 0 &amp;&amp; Start__c &lt; TODAY(), &apos;Task is starting late&apos;, &apos;Task is on time&apos;)
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The reason for the current Timeline Health status.</inlineHelpText>
        <label>Timeline Health Reason</label>
