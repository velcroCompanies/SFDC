        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CASE(Status_Enum__c,
  2, &apos;Green&apos;,
  -1, &apos;&apos;,
  IF(End__c &lt; TODAY(),
    &apos;Red&apos;,
    IF(Status_Enum__c == 0 &amp;&amp; Start__c &lt; TODAY(), &apos;Yellow&apos;, &apos;Green&apos;)
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The health of the Task as determined by the Status, Start Date, and End Date of the Task. See the &apos;Timeline Health Reason&apos; for more information.</inlineHelpText>
        <label>Timeline Health</label>
        <required>false</required>
