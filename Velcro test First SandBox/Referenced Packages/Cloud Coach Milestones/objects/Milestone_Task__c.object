    <fields>
        <fullName>Completion_Percent__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Status__c, &apos;Completed&apos;),
  1,
  IF(ISBLANK(Total_Sub_Tasks__c) || Total_Sub_Tasks__c == 0,
    0,
    Total_Completed_Sub_Tasks__c / Total_Sub_Tasks__c
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Completion Percent</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Completion_Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Completed_Date__c),
  /* If not completed date, then consider the task incomplete */
  &apos;Incomplete&apos;,
  CASE(MAX(Completed_Date__c - Start__c - Calculated_Duration__c, -1),
    -1, &apos;Early&apos;,
    0, &apos;On Time&apos;,
    &apos;Late&apos;
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Completion Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
