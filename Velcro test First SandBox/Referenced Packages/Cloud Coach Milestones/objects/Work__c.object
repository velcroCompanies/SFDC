    <validationRules>
        <fullName>Time_Entries_Timesheet_Locked</fullName>
        <active>true</active>
        <description>When the status of the Timesheet is Submitted or Approved, the hours entered on a Time Entry can&apos;t be modified</description>
        <errorConditionFormula>(ISNEW() || ISCHANGED(Hours__c)) &amp;&amp; (ISPICKVAL(Timesheet__r.Status__c, &apos;Submitted&apos;) || ISPICKVAL(Timesheet__r.Status__c, &apos;Approved&apos;))</errorConditionFormula>
        <errorMessage>Timesheet is locked. The Time Entry can not be modified.</errorMessage>
    </validationRules>
